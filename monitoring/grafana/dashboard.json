{
  "dashboard": {
    "id": null,
    "title": "MentorHub Application Monitoring",
    "timezone": "browser",
    "schemaVersion": 39,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "type": "stat",
        "title": "Overall Status",
        "gridPos": {
          "h": 4,
          "w": 4,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "up{job=\"mentorhub\"}",
            "instant": true,
            "legendFormat": "Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "color": "red",
                    "text": "DOWN"
                  },
                  "1": {
                    "color": "green",
                    "text": "UP"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "type": "stat",
        "title": "Total Requests",
        "gridPos": {
          "h": 4,
          "w": 4,
          "x": 4,
          "y": 0
        },
        "targets": [
          {
            "expr": "sum(mentorhub_requests_total)",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 3,
        "type": "stat",
        "title": "Error Rate",
        "gridPos": {
          "h": 4,
          "w": 4,
          "x": 8,
          "y": 0
        },
        "targets": [
          {
            "expr": "(sum(rate(mentorhub_errors_total[5m])) / sum(rate(mentorhub_requests_total[5m]))) * 100",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "type": "stat",
        "title": "Avg Response Time",
        "gridPos": {
          "h": 4,
          "w": 4,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(mentorhub_request_duration_seconds_bucket[5m])) by (le))",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 5,
        "type": "stat",
        "title": "Active Users",
        "gridPos": {
          "h": 4,
          "w": 4,
          "x": 16,
          "y": 0
        },
        "targets": [
          {
            "expr": "mentorhub_active_users",
            "instant": true
          }
        ]
      },
      {
        "id": 6,
        "type": "stat",
        "title": "Cache Hit Rate",
        "gridPos": {
          "h": 4,
          "w": 4,
          "x": 20,
          "y": 0
        },
        "targets": [
          {
            "expr": "(sum(mentorhub_cache_hits_total) / (sum(mentorhub_cache_hits_total) + sum(mentorhub_cache_misses_total))) * 100",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 80
                },
                {
                  "color": "green",
                  "value": 90
                }
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "type": "timeseries",
        "title": "Request Rate by Method",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        },
        "targets": [
          {
            "expr": "sum(rate(mentorhub_requests_total[5m])) by (method)",
            "legendFormat": "{{method}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "req/s"
          }
        }
      },
      {
        "id": 8,
        "type": "timeseries",
        "title": "Error Rate by Endpoint",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 4
        },
        "targets": [
          {
            "expr": "sum(rate(mentorhub_errors_total[5m])) by (endpoint)",
            "legendFormat": "{{endpoint}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "req/s"
          }
        }
      },
      {
        "id": 9,
        "type": "timeseries",
        "title": "95th Percentile Response Time",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 12
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(mentorhub_request_duration_seconds_bucket[5m])) by (le, endpoint))",
            "legendFormat": "{{endpoint}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 10,
        "type": "timeseries",
        "title": "Request Size Distribution",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 12
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(mentorhub_request_size_bytes_bucket[5m])) by (le))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(mentorhub_request_size_bytes_bucket[5m])) by (le))",
            "legendFormat": "50th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes"
          }
        }
      },
      {
        "id": 11,
        "type": "timeseries",
        "title": "Database Query Duration",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 20
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(mentorhub_database_query_duration_seconds_bucket[5m])) by (le, query_type))",
            "legendFormat": "{{query_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 12,
        "type": "timeseries",
        "title": "Database Queries by Type",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 20
        },
        "targets": [
          {
            "expr": "sum(rate(mentorhub_database_queries_total[5m])) by (query_type)",
            "legendFormat": "{{query_type}}"
          }
        ]
      },
      {
        "id": 13,
        "type": "timeseries",
        "title": "Cache Operations",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 28
        },
        "targets": [
          {
            "expr": "sum(rate(mentorhub_cache_operations_total[5m])) by (operation)",
            "legendFormat": "{{operation}}"
          }
        ]
      },
      {
        "id": 14,
        "type": "timeseries",
        "title": "External API Calls",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 28
        },
        "targets": [
          {
            "expr": "sum(rate(mentorhub_external_api_calls_total[5m])) by (service)",
            "legendFormat": "{{service}}"
          }
        ]
      },
      {
        "id": 15,
        "type": "timeseries",
        "title": "Background Tasks",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 36
        },
        "targets": [
          {
            "expr": "sum(rate(mentorhub_background_tasks_total[5m])) by (task_type, status)",
            "legendFormat": "{{task_type}} - {{status}}"
          }
        ]
      },
      {
        "id": 16,
        "type": "timeseries",
        "title": "Payment Transactions",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 36
        },
        "targets": [
          {
            "expr": "sum(rate(mentorhub_payment_transactions_total[5m])) by (status)",
            "legendFormat": "{{status}}"
          }
        ]
      },
      {
        "id": 17,
        "type": "timeseries",
        "title": "Messages Sent",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 44
        },
        "targets": [
          {
            "expr": "sum(rate(mentorhub_messages_sent_total[5m])) by (message_type)",
            "legendFormat": "{{message_type}}"
          }
        ]
      },
      {
        "id": 18,
        "type": "timeseries",
        "title": "Video Sessions",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 44
        },
        "targets": [
          {
            "expr": "sum(rate(mentorhub_video_sessions_total[5m])) by (status)",
            "legendFormat": "{{status}}"
          }
        ]
      },
      {
        "id": 19,
        "type": "timeseries",
        "title": "System Resources",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 52
        },
        "targets": [
          {
            "expr": "node_cpu_seconds_total{mode=\"idle\"}",
            "legendFormat": "CPU Idle"
          },
          {
            "expr": "node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes * 100",
            "legendFormat": "Memory Available %"
          }
        ]
      },
      {
        "id": 20,
        "type": "timeseries",
        "title": "Database Connection Pool",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 52
        },
        "targets": [
          {
            "expr": "mentorhub_db_connection_pool{pool_type=\"used\"}",
            "legendFormat": "Used Connections"
          },
          {
            "expr": "mentorhub_db_connection_pool{pool_type=\"size\"}",
            "legendFormat": "Pool Size"
          }
        ]
      }
    ],
    "templating": {
      "list": []
    },
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    }
  }
}